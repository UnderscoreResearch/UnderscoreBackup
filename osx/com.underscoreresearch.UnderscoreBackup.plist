<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN"
    "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.underscoreresearch.UnderscoreBackup</string>
    <key>KeepAlive</key>
    <true/>
    <key>ProcessType</key>
    <string>Background</string>
    <key>ProgramArguments</key>
    <array>             
        <string>HOME/Library/UnderscoreBackup/bin/UnderscoreBackup</string>
        <string>interactive</string>
    </array>
    <key>RunAtLoad</key>
    <true/>
</dict>
</plist>
